TEST Y02S - sort
SORT test/Y02.this.tst

TEST Y03 - cubic with coeff <= 2
DATA
+ (2 + 2*x + x^2 + x^3)^3
+ (1 - 2*x - x^2 - x^3)^3
+ ( - 1 - 2*x - x^2)^3
- (2 + x + 2*x^2)^3
CALL ParameterEquation -expand -f $(DATA) -l -16 -w 1 -e 3 -t Cubic.Y03
TEST Y03S - sort
SORT test/Y03.this.tst

TEST Y04 - cubic
DATA
+ (1 - 2*x - x^2 - x^3)^3
- ( - 2 - 2*x - x^2 - x^3)^3
- (1 + 2*x + x^2)^3
- (2 + x + 2*x^2)^3
CALL ParameterEquation -expand -f $(DATA) -l -16 -w 1 -e 3 -t Cubic.Y04
TEST Y04S - sort

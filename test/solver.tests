TEST T25 Fermat/Carcavi(7) x^2 + 2 = y^3; solution [5,3]; http://math.stackexchange.com/questions/677422/solve-x22-y3-using-infinite-descent; growing !
CALL TreeSolver -b 2 -l 7 -e "x^2 + 2 - y^3" -r same,invall,showfail

TEST T26 Squeeze; https://hcmop.wordpress.com/2012/04/09/techniques-in-solving-diophantine-equations-part-2/
CALL TreeSolver -b 2 -l 8 -e "x * (x + 1) * (x + 7) * (x + 8) - y^2"

TEST T27 Fermat/Carcavi(7) mod. (x+1)^2 + 2 = (y+1)^3; solution [4,2]
CALL TreeSolver -b 2 -l 5 -e "(x+1)^2 + 2 - (y+1)^3" -r transpose,same,dogrow 

TEST T28 Fermat/Carcavi(7) x^2 + 2 = y^3; solution [5,3]; http://math.stackexchange.com/questions/677422/solve-x22-y3-using-infinite-descent; growing !
CALL TreeSolver -b 3 -l 2 -e "x^2 + 2 - y^3" -r transpose,same,dogrow

TEST T29 Fermat/Carcavi(8) x^2 + 4 = y^3; solutions [2,2],[11,5]
CALL TreeSolver -b 2 -l 4 -e "x^2 + 4 - y^3" -r transpose,same,dogrow,norm,invall

TEST T30 Fermat/Carcavi(10); solutions [1,1],[2,5]; http://www.jstor.org/stable/2974148?seq=1#page_scan_tab_contents
CALL TreeSolver -b 2 -l 4 -e "(2*x^2 - 1)^2 - (2*y^2 - 1)" -r transpose,same,dogrow 

TEST T31 Fermat/Carcavi(7) x^2 + 2 = y^3; -> (a+b, a-b)
CALL TreeSolver -b 2 -l 7 -e "(a+b)^2 + 2 - (a-b)^3" -r transpose,same,norm,invall 

TEST T32 FC7, behind solution
CALL TreeSolver -b 2 -l 7 -e "(5 + 2*v + 2*u)^2 + 2 - (5 + 2*v)^3" -r same,transpose,dogrow,norm,invall

TEST T33 Oeuvres de Fermat 5, p. 154 Remarques de A. Genocchi
CALL TreeSolver -b 2 -l 4 -e "x^4 + x^2*y^2 + y^4 - z^2" -r same,transpose,dogrow,norm,invall

TEST T34 Keith Conrad Descent: Cor. 3.7
CALL TreeSolver -b 2 -l 4 -e "y^2 = x^3 - 4*x" -r same,transpose,dogrow,norm,invall

TEST T35 FC7
CALL TreeSolver -b 2 -l 5 -e " - 49 - 65*u - 28*u^2 - 4*u^3 + 75*v + 60*u*v + 12*u^2*v - 30*v^2 - 12*u*v^2 + 4*v^3" -r same,transpose,similiar,norm,invall

TEST T36 area of a rational triangle

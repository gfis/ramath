TEST PF4 PolyFraction -f sage (g.f.)
DATA
 A000008  sage  1 ((1-x)*(1-x^2)*(1-x^5)*(1-x^10))
 A000012  sage  1 (1-x)
 A000027  sage  x (1-x)^2
 A000032  sage  (2-x) (1-x-x^2)
CALL PolyFraction -f $(DATA)

TEST PF5 PolyFraction -f coxg
DATA
 A169452  coxf  33 7
 A162740  coxf  3 4
 A162756  coxf  3 10
 A162920  coxf  4 3
 A170100  coxf  37 43
 A170731  coxf  50 50

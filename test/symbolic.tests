TEST RR1 Rest - univariate
CALL RelationSet -rest 2 "-1+2*x+12*x^2+32*x^3+32*x^4-32*y^4-3*z-2*z^2=0"

TEST RR4 Rest - all polynomials
CALL RelationSet -rest 2 "x^4 - y^4 - z^2 = 0"


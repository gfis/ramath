CALL BaseReason -same \
  -rset0 "2a^2 - 2b + 2" \
  -rset1 "2a^2 - 2b + 2" \
  -rset2 "4a^2 - 4b + 4"

TEST RES2 SameReason: same with map
CALL BaseReason -same \
  -rset0 "a^2+b^2-c^2-d^2; -a+w^2;  -b+ x^2; -c+ y^2; -d+ z^2" \
  -rset1 "a^2+b^2-c^2-d^2; -a+w^2;  -b+ x^2; -c+ y^2; -d+ z^2" \
